{
  "name": "dejoaromaswebpage",
  "version": "1.0.0",
  "description": "Jiovanni Go - Perfumer√≠a completa",
  "main": "index.js",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:ngrok": "ngrok start --all --config=ngrok-config.yml",
    "start:ngrok:bat": "start-ngrok.bat",
    "start:ngrok:ps": "powershell -ExecutionPolicy Bypass -File start-ngrok.ps1",
    "build:frontend": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "mongoose": "^8.19.4",
    "telnet": "^0.0.1"
  }
}
